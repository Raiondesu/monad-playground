{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "./src",
    "downlevelIteration": true,
    "target": "es2015",
    "strict": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext"
